category frågetecken {
info("Användning av frågetecken")
link("" "")
}

frågeordsfråga@frågetecken
{
X1(sed=sen | cht=mid),
X2(text="vem" | text="vad" | text="vart" | text="var" | text="varifrån" | text="vilka" | text="vilken"  | text="hur" | text="varför"  ),
X(wordcl!=pn & text!="som"),
X3()*, 
X4(text="." | text="!"),
X5(sed=sen) 
-->
mark( X1 X2 X X4)
corr(X4.replace("?"))
info("När frågeord så som "italics(X2.real_text)" inleder en sats kan meningen uppfattas som en fråga.")
action(scrutinizing)
}

janejfråga@frågetecken
{
X(sed=sen | (cht=mid & text!="'")),
Y(wordcl=vb & (vbf = prt | vbf = prs) & voice!=sfo),
Y2(wordcl!=pn & text!="som"),
X1(cht!=mid)+, 
X2(text="." | text="!"),
X3(sed=sen) 
-->
mark(X Y X2)
corr(X2.replace("?"))
info("Ord i presens/preterium ("italics(X2.real_text)") som inleder en sats brukar ofta innebära att meningen är en fråga.")
action(scrutinizing)
}

eller@frågetecken
{
X(text="eller" | text="eller hur"),
X2(text="." | text="!"),
X3(sed=sen) 
-->
mark(X X2)
corr(X2.replace("?"))
info("Fråga?")
action(scrutinizing)
}

närföljtavverb@frågetecken
{
X(sed=sen),
Y(text="när"),
Z(wordcl=vb & (vbf = prt | vbf = prs)),
X1()*, 
X2(text="." | text="!"),
X3(sed=sen) 
-->
mark(Y Z X2)
corr(X2.replace("?"))
info("Fråga?")
action(scrutinizing)
}

