category kompsuffix{
info("Komparation av adjektiv")
link("" "")
}

category kompperi{
info("Komparation av adjektiv")
link("" "")
}


kompsuffix@kompsuffix
{
X1(wordcl=vb),
C(wordcl=ab)?,
Z(text="mer" | text="mest" | text="mera")?,
X2(wordcl=jj & deg!= kom & substr(length-2,length)!="ad" & substr(length-3,length)!="isk"),
X3(text="än")
-->
mark(X1 C Z X2 X3)
corr(if Z.text="mer" | Z.text="mest" then Z.delete() X2.form(deg:=kom) else Z.donothing() X2.form(deg:=kom) end)
info("använd den komparativa formen av adjektivet (suffixkomparation)")
action(scrutinizing)
}

långa@kompperi
{
X1(wordcl=vb),
C(wordcl=ab)?,
X2(wordcl=jj & (substr(length-5,length)="adare" | substr(length-6,length)="iskare") |
substr(length-5,length)="ndare")
-->
mark(X1 C X2)
corr(if X2.substr(X2.length-5,X2.length)="ndare" then X2.insert("mera") X2.replace(concat(X2.substr(0, X2.length-3), "e ."))
else X2.insert("mera") X2.replace(concat(X2.substr(0, X2.length-3), "")) end) 
info("Långa adjektiv böjs med 'mera' (perfifrastiskt komparation)")
detect("Han är förvånadare än mig.")
action(scrutinizing)
}

lik@kompperi
{
X1(wordcl=vb),
C(wordcl=ab)?,
X2(text="likare")
-->
mark(X1 C X2)
corr(X2.insert("mera") X2.replace("lik")) 
info("'lik' böjs med 'mera' (perfifrastiskt komparation)")
action(scrutinizing)
}

värd@kompperi
{
X1(wordcl=vb),
C(wordcl=ab)?,
X2(text="värdare")
-->
mark(X1 C X2)
corr(X2.insert("mera") X2.replace("värd")) 
info("'värd' böjs med 'mera' (perfifrastiskt komparation)")
action(scrutinizing)
}


